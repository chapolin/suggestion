<div id="page-content">
    <div class="container">
        <div class="row">
            <div id="first-step" class="col-xs-12">
                <form class="first-step">
                <fieldset>
                <div class="form-group">
                    <div class="col-xs-12 prefetch-step-1">
                        <h2>Qual serviço vai usar?</h2>
                        <div class="content-message">
                        	<span class="warning-message"></span>
	            			<span class="arrow-down-message"></span>
	            			
                        	<input name="_service" class="typeahead tt-input form-control input-lg" type="text">
                        	<button type="button" class="btn btn-lg btn-primary btn-block btn-continue btn-step-2">Continuar</button>
                       	</div>
                    </div>
                </div>
                </fieldset>
                </form>
                <div class="container">
                    <div class="row">
                        <div class="col-xs-12"><h2>Serviços em destaque</h2></div>
                    </div>
                    <div class="row">
                        <div class="col-xs-12">
                            <a class="promotion" href="#">
                                <div class="heading">
                                    <h4 class="title">
                                        Cambagem por R$ 22,00
                                    </h4>
                                </div>
                            </a>
                            <a class="promotion" href="#">
                                <div class="heading">
                                    <h4 class="title">
                                        Diarista por R$ 35,00
                                    </h4>
                                </div>
                            </a>
                            <a class="promotion" href="#">
                                <div class="heading">
                                    <h4 class="title">
                                        Banho e tosa por R$ 80,00
                                    </h4>
                                </div>
                            </a>
                        </div>
                    </div>
                </div>
            </div>
            <div id="second-step" class="col-xs-12">
                <form class="second-step">
                <fieldset>
                <div class="form-group">
                    <div class="col-xs-12">
                        <h2>Qual valor tem em mãos?</h2>
                        <div class="input-group">
                            <div class="input-group-addon">R$</div>
                            <input type="tel" name="_value" class="form-control input-lg input-price money" data-thousands="." data-decimal="," placeholder="0,00">
                        </div>
                    </div>
                </div>

                <div class="form-group">
                    <div class="col-xs-12 prefetch-step-2">
                        <h2>Onde?</h2>
                        <div class="form-group">
                            <input name="_place" class="typeahead tt-input form-control input-lg" type="text">
                            <button type="button" class="btn btn-lg btn-primary btn-block btn-search btn-step-3">Buscar</button>
                        </div>
                    </div>
                </div>
                </fieldset>
                </form>
            </div>
            <div id="third-step" class="col-xs-12">
                <form class="form-horizontal">
                <fieldset>
                <h2>Balanceamento</h2>
                <div class="panel-group" id="accordion" role="tablist" aria-multiselectable="true">

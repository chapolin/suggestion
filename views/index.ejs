</div>
<div id="page-content">
    <div class="container">
        <div class="row">
<!-- ### FIRST STEP ### -->        
            <div id="first-step" class="col-xs-12">
                <form class="first-step">
                <fieldset>
                <div class="form-group">
                    <div class="col-xs-12">
                        <h2>Qual serviço vai usar?</h2>
                        <div class="input-group">
                            <div id="prefetch">
                                <input class="typeahead tt-input form-control" type="text" placeholder="Serviço a cotar">
                            </div>
                            <div class="input-group-btn">
                                <button id="btnNext" class="btn btn-default" type="submit"><i class="glyphicon glyphicon-arrow-right"></i></button>
                            </div>
                        </div>
                    </div>
                </div>

                </fieldset>
                </form>
            </div>
<!-- ### TWO STEP ### -->
            <div id="second-step" class="col-xs-12 hidden">
                <form class="second-step">
                <fieldset>
                <div class="form-group">
                    <div class="col-xs-12">
                        <h2>Qual valor tem em mãos?</h2>
                        <div class="input-group">
                            <div class="input-group-addon">R$</div>
                            <input type="text" class="form-control" placeholder="00.00" name="price" id="price">
                        </div>
                    </div>
                </div>

                <div class="form-group">
                    <div class="col-xs-12">
                        <h2>Onde?</h2>
                        <div class="input-group">
                            <input type="text" class="form-control" placeholder="Nome do estabelecimento" name="name-establishment" id="name-establishment">
                            <div class="input-group-btn">
                                <button id="btnNext" class="btn btn-default" type="submit"><i class="glyphicon glyphicon-arrow-right"></i></button>
                            </div>
                        </div>
                    </div>
                </div>
                </fieldset>
                </form>
            </div>
<!-- ### THIRD STEP ### -->
